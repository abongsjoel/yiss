{
  "name": "yiss-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "docz": "^2.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:styles": "postcss src/tailwind.css -o src/styles.css",
    "watch:styles": "postcss src/tailwind.css -o src/styles.css --watch",
    "prebuild": "npm run build:styles",
    "prestart": "npm run build:styles",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.1.4"
  }
}
